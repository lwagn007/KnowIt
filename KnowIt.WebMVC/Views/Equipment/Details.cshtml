@model KnowIt.Models.Equipment.EquipmentDetail

@{
    ViewBag.Title = "Details";
}


<div class="panel transparent" style="overflow:hidden">
    <div class="tablespace">
        <h2>Equipment Details</h2>
        <p>
            @Html.ActionLink("Edit", "Edit", new { id= Model.EquipmentId }) |
            @Html.ActionLink("Back to List", "Index")
        </p>
        <hr />
        <div class="panel panel-body panel-info">
            <dl class="dl-horizontal">
                <dt>
                    Equipment Name:
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.EquipmentName)
                </dd>
                <br/>
                <dt>
                    Equipment Note:
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.EquipmentNote)
                </dd>
            </dl>
        </div>
    </div>
</div>
