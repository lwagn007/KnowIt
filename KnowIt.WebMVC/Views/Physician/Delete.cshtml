@model KnowIt.Models.Physician.PhysicianDetail

@{
    ViewBag.Title = "Delete";
}

<div class="panel panelsize" style="overflow:hidden">
    <div class="tablespace">
        <h2>Delete</h2>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" /> |
                @Html.ActionLink("Back to List", "Index")
            </div>
        }
        <hr />
        <div class="panel panel-body panel-info">
            <dl class="dl-horizontal">
                <dt>
                    Physician First Name:
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.PhysicianFirstName)
                </dd>
                <br />
                <dt>
                    Physician Last Name:
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.PhysicianLastName)
                </dd>
                <br />
                <dt>
                    Physician Specialty:
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Specialty)
                </dd>

            </dl>
        </div>
    </div>
</div>
