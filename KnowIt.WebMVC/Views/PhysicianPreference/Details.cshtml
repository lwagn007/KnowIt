@model KnowIt.Models.PhysicianProcedure.PhysicianPreferenceDetail

@{
    ViewBag.Title = "Details";
}


<div class="panel panelsize">
    <div class="tablespace">
        <h2>Physician Preference Detail</h2>
        <p>
            @Html.ActionLink("Back to List", "Index")
        </p>
        <hr />
        <div class="panel panel-body panel-info">
            <dl class="dl-horizontal ">
                <dt>
                    Physician Last Name:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.PhysicianLastName)
                </dd>
                <br />
                <dt>
                    Physician First Name:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.PhysicianFirstName)
                </dd>
                <br />
                <dt>
                    Specialty:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Specialty)
                </dd>
                <br />
                <dt>
                    Procedure Name:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.ProcedureName)
                </dd>
                <br />
                <dt>
                    Procedure Notes:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.ProcedureNote)
                </dd>
                <br />
                <dt>
                    Procedure Route:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.ProcedureRoute)
                </dd>
                <br />
                <dt>
                    Medication:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.MedicationName)
                </dd>
                <br />
                <dt>
                    Equipment:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.EquipmentName)
                </dd>
                <br />
                <dt>
                    Physician Preference Notes:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.PreferenceNote)
                </dd>
            </dl>
        </div>
    </div>
</div>
